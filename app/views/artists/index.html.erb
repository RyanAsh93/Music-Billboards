<h1>Artists#index</h1>
<%= link_to "Add Artist", new_billboard_artist_path(@billboard) %>
<% @artists.each do |artist| %>
  <h2> <%= artist.name %> </h2>
  <%= link_to "View", artists_path %>
  <%= link_to "Edit Artist", edit_artist_path(artist) %>
  <%= link_to "Delete Artist", artists_path(billboard), method: :delete %>

<% end %>
